<body>
<header>
   <nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/">SPORTSERA</a>
    </div>
    <ul class="nav navbar-nav">
      <li><a href="/">HOME</a></li>
    </ul>
  </div>
</nav>
  </header>

<!--<p>hello</p>-->
<% console.log(places); %>
<!--  if(places[0]["gid"]==101) { %> -->
<!-- <a href="/booking">ok</a> -->
<!--  } %> -->

<script>alertify.error("Venue has already been booked!! Choose another one.");</script>

<table class="content-table">
    <thead>
    <tr>
        <th>Serial No.</th>
        <th>Ground Name</th>
        <th>Location</th>
        <th>Rating</th>
        <th>Timing</th>
    </tr>
    </thead>
    <tbody>
    <%
        for (var i = 0;i<places.length;++i) {
    %>
    <tr>
        <td><%=i+1; %></td>
        <td><a href="/booking?gid=<%=places[i]["gid"]%>"><%=JSON.stringify(places[i]["groundname"])%></a></td>
        <td><%=JSON.stringify(places[i]["location"])%></td>
        <td><%=JSON.stringify(places[i]["rating"])%></td>
        <td><% if(places[i]["tid"]==1) { %>
            8:00 am
            <% } else if(places[i]["tid"]==2) { %>
            12:00 pm
            <% } else if(places[i]["tid"]==3) { %>
            4:00 pm
            <% } else if(places[i]["tid"]==4) { %>
            8:00 pm
            <% } %>
         </td>
    </tr>
    <% } %>
   </tbody>
 </table>
</body>
</html>